{
  "name": "tiffin-management",
  "version": "0.0.1",
  "description": "Tiffin Management System - Phase 3 (Microservices)",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start:db": "docker-compose up -d db",
    "setup": "npm run migrate && npm run seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest.config.js",
    "test:auth": "jest apps/auth-service/test",
    "test:user": "jest apps/user-service/test",
    "test:meal": "jest apps/meal-service/test",
    "test:admin": "jest apps/admin-service/test",
    "test:api": "bash test-api.sh",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "seed": "ts-node prisma/seed.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "kill:port": "kill-port.bat",
    "prisma:push": "npx prisma db push"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^2.0.3",
    "@nestjs/axios": "^3.0.1",
